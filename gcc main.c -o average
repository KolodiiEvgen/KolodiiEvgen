#include <stdio.h>

int main() {
    int N;
    int number;
    int sum = 0;
    int count = 0;

    // Ввод количества входных чисел
    scanf("%d", &N);

    // Ввод и обработка чисел
    for (int i = 0; i < N; i++) {
        scanf("%d", &number);
        if (number > 0) {
            sum += number;
            count++;
        }
    }

    // Проверка на случай, если N равно нулю
    if (count == 0) {
        printf("0\n");
    } else {
        // Вычисление и вывод среднего арифметического
        float average = (float)sum / count;
        printf("%.2f\n", average);
    }

    return 0;
}
